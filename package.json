{
  "name": "rewq",
  "version": "1.0.0",
  "description": "A bookmarking tool for kindle paperwhite",
  "main": "app.js",
  "scripts": {
    "local": "NODE_ENV=local node app.js",
    "start": "node app.js",
    "test": "(cd interfaces/webapp; npm run test;)",
    "deploy-dev": "MAKE_ENV=dev make deploy",
    "deploy-prod": "MAKE_ENV=prod make deploy",
    "logs": "gcloud app logs read",
    "list": "./scripts/list_all_modules.sh",
    "quickly": "mongod & npm run local",
    "local-docker": "docker-compose -f ./scripts/docker-compose.yml -p dimagi run webapp-local",
    "import-testdata": "mongoimport --db dimagi --collection users --jsonArray test/test-user-data.json"
  },
  "engines": {
    "node": "v6.2.2"
  },
  "license": "ISC",
  "dependencies": {
    "archiejs": "^0.3.1",
    "archiejs-kue-enhancer": "^0.4.0",
    "archiejs-mongo-enhancer": "^0.3.0",
    "assert": "^1.4.1",
    "async": "^1.5.2",
    "body-parser": "^1.15.2",
    "command-line-args": "git+https://github.com/flipflopapp/command-line-args.git",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "es6-promise-pool": "^2.4.2",
    "es6-promisify": "^4.1.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "helmet": "^2.1.1",
    "hjson": "^1.8.4",
    "http": "0.0.0",
    "immutable": "^3.8.1",
    "jsonschema": "^1.1.0",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.3",
    "mongoose-geojson-schema": "^2.1.2",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
